<xar:set name="compare">xarServerGetCurrentURL()</xar:set>
<xar:set name="tabclass">'inactive'</xar:set>
<xar:if condition="$compare eq $current">
    <xar:set name="tabclass">'active'</xar:set>
</xar:if>

<dd class="#$tabclass#">
    <a href="#$current##">
        <xar:mlstring>
            Select Theme
        </xar:mlstring>
    </a>
</dd>    
