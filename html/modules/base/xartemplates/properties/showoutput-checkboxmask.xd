<xar:if condition="$numOptionsSelected eq 0">
    <em>None Specified</em>
    <xar:else />
    <xar:foreach in="$options" value="$option">
        <xar:if condition="$option['checked']">
        <strong>#$option['id']#</strong>- #$option['name']#
        <br />
        <xar:else />
        <!-- #$option['id']# - #$option['name']#
        <br /> --> </xar:if>
    </xar:foreach>
</xar:if>
