<?xml version="1.0" encoding="utf-8"?>
<xar:template xmlns:xar="http://xaraya.com/2004/blocklayout">
    <xar:if condition="!empty($number)">
        <xar:foreach in="$columns" key="$col">
            <xar:foreach in="$columns[$col]" key="$row">#$columns[$col][$row]#</xar:foreach>
        </xar:foreach>
    </xar:if>
</xar:template>