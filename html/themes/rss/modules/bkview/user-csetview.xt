<xar:template xmlns:xar="http://xaraya.com/2004/blocklayout">
<xar:loop name="$csets">
<item>
    <title>ChangeSet #$loop:item['rev']#</title>
    <link>&xar-modurl-bkview-user-deltaview;&amp;rev=#$loop:item['rev']#&amp;repoid=#$repoid#</link>
    <description>#$loop:item['comments']#</description>
    <author>#$loop:item['author']#</author>
    <pubDate>#xarLocaleFormatUTCDate("%a, %d %b %Y %H:%M:%S GMT",$loop:item['utc'])#</pubDate>
</item>
</xar:loop></xar:template>
