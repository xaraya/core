includes:
    - phpstan-baseline.neon

parameters:
    level: 0
    # Dynamic properties emits warnings from PHP 8.2 onwards.
    # checkDynamicProperties: true
    paths:
        - html
        - developer
    bootstrapFiles:
        - phpstan-bootstrap.php
    excludePaths:
        - html/var/*
        - html/lib/creole/*
        - html/lib/xaraya/legacy/*
    parallel:
        processTimeout: 300.0
